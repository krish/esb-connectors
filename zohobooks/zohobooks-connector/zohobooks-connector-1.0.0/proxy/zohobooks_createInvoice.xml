            <exchangeRate>{$ctx:exchangeRate}</exchangeRate>
            <recurringInvoiceId>{$ctx:recurringInvoiceId}</recurringInvoiceId>
            <invoicedEstimateId>{$ctx:invoicedEstimateId}</invoicedEstimateId>
