<component name="mailchimp-list" type="synapse/template">
   <subComponents>
      <component name="removeSubscribersFromList">
         <file>removeSubscribersFromList.xml</file>
         <description>Unsubscribe a batch of email addresses from a list at once.</description>
      </component>
      <component name="addSubscribersToList">
         <file>addSubscribersToList.xml</file>
         <description>Subscribe a batch of email addresses to a list at once.</description>
      </component>
      <component name="listLists">
         <file>listSubscriberLists.xml</file>
         <description>Retrieve all of the lists defined for your user account.</description>
      </component>
   </subComponents>
</component>